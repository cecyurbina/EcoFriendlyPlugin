{
  "manifest_version": 2,
  "name": "A jQuery Chrome extension",
  "version": "0.1",
  "description": "Use jQuery to build Chrome extensions",
  "permissions": ["tabs","<all_urls>", "file:///*"],
  "web_accessible_resources": ["file:///Users/cecyurbina/Downloads/tesis/dist/fonts/glyphicons-halflings-regular.eot",
			       "file:///Users/cecyurbina/Downloads/tesis/dist/fonts/glyphicons-halflings-regular.svg",
			       "file:///Users/cecyurbina/Downloads/tesis/dist/fonts/glyphicons-halflings-regular.ttf",
			       "file:///Users/cecyurbina/Downloads/tesis/dist/fonts/glyphicons-halflings-regular.woff"],

  "content_scripts": [
